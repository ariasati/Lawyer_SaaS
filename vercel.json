{
  "functions": {
    "public/index.html": {
      "memory": 128
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/signin",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/pricing",
      "dest": "/index.html"
    },
    {
      "src": "/features",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "trailingSlash": false
} 